# Exemple de configuration des secrets pour Streamlit Cloud et local
# Copiez ce contenu dans .streamlit/secrets.toml (ne pas commiter ce fichier !)

[postgres]
url = "postgresql://postgres:[PASSWORD]@db.[PROJECT_REF].supabase.co:5432/postgres"

# Alternativement, vous pouvez utiliser SUPABASE_DB_URL
# SUPABASE_DB_URL = "postgresql://..."

# Clés API pour les modèles IA
[api_keys]
openai = "sk-..."
anthropic = "sk-..."
openrouter = "sk-..."
